@section scripts{
    <script src="~/Content/js/slick.min.js"></script>
    <script>
        function Download(type) {
            if (type == "Excel") {
                DownloadExcel();
            }
            else if (type == "Image") {
                DownloadMedia(type);
            }
            else if (type == "Video") {
                DownloadMedia(type);
            }
            else if (type == "Certificate") {
                DownloadMedia(type);
            }
        }

        function DownloadExcel() {
            $('.loading-overlay-image-container').show();
            $('.loading-overlay').show();
            var stoneId = $("#hdnStoneRefNo").val();
            alert(stoneId);
            $.ajax({
                url: "/Common/StockExcelDownloadByStoneId",
                type: "POST",
                data: { StoneID:  stoneId},
                success: function (data, textStatus, jqXHR) {
                    location.href = data;
                    $('.loading-overlay-image-container').hide();
                    $('.loading-overlay').hide();
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $('.loading-overlay-image-container').hide();
                    $('.loading-overlay').hide();
                }
            });
        }

        function DownloadMedia(type) {

            $('.loading-overlay-image-container').show();
            $('.loading-overlay').show();
            var stoneId = $("#hdnStoneRefNo").val();
            $.ajax({
                url: "/Common/StockMediaDownloadByStoneId",
                type: "POST",
                data: { StoneID: stoneId, MediaType: type },
                success: function (data, textStatus, jqXHR) {
                    if (data.search('.zip') == -1 && data.search('.pdf') == -1) {
                        toastr.error(data);
                    } else {
                        location.href = data;
                    }
                    $('.loading-overlay-image-container').hide();
                    $('.loading-overlay').hide();
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $('.loading-overlay-image-container').hide();
                    $('.loading-overlay').hide();
                }
            });
        }

        function formatNumber(number) {
            return (parseFloat(number).toFixed(2)).toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,");
        }
        jQuery(document).ready(function ($) {
            if ($("#liRapPrice").text() != "") {
                $("#liRapPrice").text(formatNumber($("#liRapPrice").text()));
            }

            if ($("#liRapValue").text() != "") {
                $("#liRapValue").text(formatNumber($("#liRapValue").text()));
            }

            if ($("#liTotal").text() != "") {
