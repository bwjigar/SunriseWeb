<div class="modal fade social-model" id="DeleteModal">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Delete Offer</h4>
                <button type="button" class="close" onclick="ClearDeleteRemoveModel()">&times;</button>
            </div>
            <div class="modal-body">
                <li>Are you sure you want to delete this Offer</li>
            </div>

            <div class="modal-footer">
                <button id="btnyes" class="btn btn-secondary" onclick="DeleteOfferFinal()" type="button">Delete</button>
                <button id="btnno" class="btn btn-primary" type="button" onclick="ClearDeleteRemoveModel()">@LanguageProvider.get("cancel")</button>
            </div>
        </div>
    </div>
