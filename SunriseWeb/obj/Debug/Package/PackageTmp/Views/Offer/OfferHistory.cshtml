<div class="container-fluid order-history-section">
    <div class="row">
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="order-title">
                <h2>@LanguageProvider.get("Offer_History")</h2>
            </div>
        </div>
    </div>
    <div class="order-history-data admin">
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
                <div class="col-xl-4 col-lg-6 col-md-6 col-sm-6 col-12 float-left pl-2 pr-2 mb-2">
                    <div class="order-date common-order">
                        <div class="offer-label">@LanguageProvider.get("offerdate"):</div>
                        <input type="text" name="From" id="txtFromDate" autocomplete="off" value="" class="offer-input offer-input1" />
                        <input type="text" name="To" id="txtToDate" autocomplete="off" value="" class="offer-input offer-input1" />
                    </div>
                </div>
                @if (SunriseWeb.Helper.SessionFacade.UserSession.isadmin == 1 || SunriseWeb.Helper.SessionFacade.UserSession.isemp == 1)
                {
                    <div class="col-xl-4 col-lg-6 col-md-6 col-sm-6 col-12 float-left pl-2 pr-2 mb-2">
                        <div class="order-company-name common-order">
                            <div class="offer-label">@LanguageProvider.get("Company"):</div>
                            <input id="txtCompanyName" placeholder="@LanguageProvider.get("CompanyName") / @LanguageProvider.get("username")" type="text" class="form-control common-control">
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-md-6 col-sm-6 col-12 float-left pl-2 pr-2 mb-2">
                        <div class="order-stone-id common-order">
                            <div class="offer-label">@LanguageProvider.get("Stock_Id"):</div>
                            <input id="txtStoneId" class="form-control common-control" placeholder="@LanguageProvider.get("Stock_Id") / @LanguageProvider.get("Offer_Id")" type="text">
                        </div>
                    </div>
                }
                else
                {
                    <div id="DivOption" class="col-xl-2 col-lg-8 col-md-7 col-sm-12 row" style="padding-left: 20px;">
                        <span class="btn-spn-opt" onclick="ActiveOrNot('Active');" id="Active">Active</span>
                        <span class="btn-spn-opt" onclick="ActiveOrNot('InActive');" id="InActive">In Active</span>
                    </div>
                }
