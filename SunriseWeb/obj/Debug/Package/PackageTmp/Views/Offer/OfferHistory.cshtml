<style type="text/css">
    .ag-paging-page-summary-panel {
        left: 10px;
        position: relative;
    }

    .offer-input1 {
        width: 95px;
    }

    .dis_none {
        display: none;
    }

    .dis_block {
        display: block;
    }

    @@media screen and (min-width: 321px) and (max-width: 411px) {
        .offer-input1 {
            width: 83px;
        }
    }

    @@media(max-width: 320px) {
        .offer-input1 {
            width: 90px;
        }
    }

    @@media screen and (min-width: 412px) and (max-width: 414px) {
        .offer-input1 {
            width: 83px;
        }
    }

    @@media screen and (min-width: 415px) and (max-width: 450px) {
    }

    @@media (width: 768px) {
        .offer-input1 {
            width: 110px;
        }
    }

    .g-head-t {
        font-size: 12px;
        padding: 0 11px;
        color: grey;
    }

    .g-amt-t {
        font-size: 12px;
        padding: 0 12px;
        color: #003d66;
    }

    #overlay {
        position: fixed;
        display: none;
        width: 100%;
        height: 100%;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-color: rgba(0,0,0,0.5);
        z-index: 2;
        cursor: pointer;
    }

    .btn-spn-opt:hover {
        background-color: #80808017;
        border: 1px solid #003d66;
    }

    .btn-spn-opt {
        border: 1px solid #e5e5e5;
        border-radius: 28px;
        padding: 5px 15px;
        float: left;
        margin-bottom: 5px;
        margin-right: 5px;
        transition-duration: 0.3s;
        -webkit-transition: 0.3s;
        cursor: pointer;
        font-size: 11px;
        white-space: nowrap;
        font-weight: 600;
        background: #fff;
        color: #003d66;
        position: relative;
        z-index: 0;
        overflow: hidden;
        margin-top: 5px;
    }

    .btn-spn-opt-active {
        color: #fff !important;
        background-color: #003d66 !important;
    }

    .spn-Yes {
        font-weight: 600;
        color: #008000f2;
        letter-spacing: 1px;
    }

    .spn-No {
        font-weight: 600;
        color: #8b0000cc;
        letter-spacing: 1px;
    }

    .spn-bg-No {
        background-color: #ffe1e1;
    }

    .spn-bg-Yes {
        background-color: #cae4cb;
    }

    .spn-Yes1 {
        border: 1px solid #e5e5e5;
        border-radius: 10px;
        padding: 0px 5px 0px 5px;
        font-size: 11px;
        font-weight: 600;
        background: #28a745;
        color: white;
    }

    .spn-No1 {
        border: 1px solid #e5e5e5;
        border-radius: 10px;
        padding: 0px 5px 0px 5px;
        font-size: 11px;
        font-weight: 600;
        background: #ef0808;
        color: white;
    }


    .showchk {
        display: block !important;
    }

    .search-page-content ul.search li a.active .icon-image {
        background: #003d66 !important;
    }

    ul.search li a:hover .icon-image {
        background: #7ba8c5 !important;
    }

    .search-page-content .common-li li.active::before {
        background: #003d66; /*rgba(0, 61, 102,0.8);*/
    }

    .search-page-content .common-li li::before {
        background: #7ba8c5;
    }

    .grid_prewrap {
        white-space: pre-wrap !important;
    }
</style>
