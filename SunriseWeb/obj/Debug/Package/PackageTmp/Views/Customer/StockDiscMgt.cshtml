            if (fcarat == "" && tcarat == "" || fcarat == 0 && tcarat == 0) {
                toastr.warning("Please Enter Carat !!");
                return false;
            }
            if (fcarat == "") {
                fcarat = "0";
            }
            var _pointerlst_ = [];
            _pointerlst_.push({
                "iSr": _pointerlst.length + 1,
                "sName": fcarat + '-' + tcarat,
                "isActive": true,
            });

            var lst = _.filter(_pointerlst, function (e) { return (e.sName == _pointerlst_[0].sName) });
