@using SunriseWeb.Resources;
@{
    ViewBag.Title = "CustomerDiscReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ClassName = "search-contnt";
}

<input type="hidden" id="hdn_Remove" value="@LanguageProvider.get("Remove")" />

<div class="container-fluid order-history-section">
    <div class="row">
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="order-title">
                <h2>Overseas Discount Report</h2>
            </div>
        </div>
    </div>
    <div class="order-history-data admin muser">
        <form id="frm">
            <div class="col-xl-5 col-lg-5 col-md-7 col-sm-6 col-12 float-left pl-2 pr-2">
                <div class="common-order">
                    <div class="offer-label">User Name:</div>
                    <input type="text" id="txtCommonName" class="form-control common-control">
                </div>
            </div>
            <div class="col-xl-3 col-lg-7 col-md-7 col-sm-6 col-12 float-left">
                <ul class="offer-inbtn p-0 mb-2">
                    <li>
                        <button type="button" id="btnSearch" class="offer-btn"><i class="fa fa-search" aria-hidden="true"></i>Search</button>
                    </li>
                    <li>
                        <button type="button" id="btnReset" class="offer-btn"><i class="fa fa-refresh" aria-hidden="true"></i>Reset</button>
                    </li>
                </ul>
            </div>
        </form>
    </div>
    <div class="aggrid-section">
        <div id="myGrid" style="width: 100%; height: 400px;" class="ag-theme-balham">
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Content/js/moment.min.js"></script>
    <script src="~/Content/js/daterangepicker.min.js"></script>
    <script src="~/Content/js/ag-grid-enterprise.min.js"></script>
    <script src="~/Content/js/common.js"></script>
    <script src="~/Content/js/Pages/detailCellRenderer.js"></script>
    <script src="~/Content/js/Pages/CustomerDiscReport.js"></script>
}

