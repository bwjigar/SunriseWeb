<!--- start of order-history part---->
<div class="container-fluid order-history-section">
    <div class="row">
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="order-title">
                <h2>Hold Stone Report</h2>
            </div>
        </div>
    </div>
    <div class="order-history-data admin">
        <div class="row">
            @if (SunriseWeb.Helper.SessionFacade.UserSession.iUserType == 1)
            {
                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
                    <div class="col-xl-5 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                        <div class="order-company-name common-order">
                            <div class="offer-label">Hold Party Name:</div>
                            <input id="txtCompanyName"
                                   placeholder="Hold Party Name / Hold Assist By / Hold Party Code"
                                   type="text" class="form-control common-control" autocomplete="off">
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                        <div class="order-stone-id common-order">
                            <div class="offer-label">Stock ID:</div>
                            <input id="txtStoneId" class="form-control common-control" placeholder="Stock ID / Certi No." type="text" autocomplete="off">
                        </div>
                    </div>
                </div>
            }
            else
            {
                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
                    <div class="col-xl-5 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                        <div class="order-company-name common-order">
                            <div class="offer-label">Hold Party Name:</div>
                            <input id="txtCompanyName"
                                   placeholder="Hold Party Name / Hold Party Code"
                                   type="text" class="form-control common-control" autocomplete="off">
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6 col-12 float-left mb-2">
                        <div class="order-stone-id common-order">
                            <div class="offer-label">Stock ID:</div>
                            <input id="txtStoneId" class="form-control common-control" placeholder="Stock ID / Certi No." type="text" autocomplete="off">
                        </div>
                    </div>
                </div>
            }
            <div id="DivTCount" class="col-xl-6 col-lg-6 col-md-6 col-sm-6">
                <div id="tab1TCount" class="row" style="display:none;margin-top: -2px;margin-bottom: -10px;padding: 0px 10px;">
                    <div><center><span class="g-head-t">@LanguageProvider.get("pcs1")</span></center><center><span id="tab1pcs" class="g-amt-t"></span></center></div>
                    <div><center><span class="g-head-t">@LanguageProvider.get("cts")</span></center><center><span id="tab1cts" class="g-amt-t"></span></center></div>
                    <div><center><span class="g-head-t">@LanguageProvider.get("Offer_Disc_Per")</span></center><center><span id="tab1disc" class="g-amt-t"></span></center></div>
                    <div><center><span class="g-head-t">@LanguageProvider.get("Offer_Value_Dollar")</span></center><center><span id="tab1totAmt" class="g-amt-t"></span></center></div>
                    <div id="tab1_WebDisc_t" style="display:none;"><center><span class="g-head-t">@LanguageProvider.get("Web_Disc_Dollar")</span></center><center><span id="tab1Web_Disc" class="g-amt-t" style="color: blue;"></span></center></div>
                    <div id="tab1_FinalValue_t" style="display:none;"><center><span class="g-head-t">@LanguageProvider.get("Final_Disc_Per")</span></center><center><span id="tab1Final_Disc" class="g-amt-t" style="color: red;"></span></center></div>
                    <div id="tab1_FinalDisc_t" style="display:none;"><center><span class="g-head-t">@LanguageProvider.get("Final_Value")</span></center><center><span id="tab1Net_Value" class="g-amt-t" style="color: red;"></span></center></div>
                </div>
            </div>
            <div class="col-xl-6 col-lg-4 col-md-3 col-sm-12">
                <ul class="offer-inbtn pull-right mrtop src-rest-topbtn mt-2 mb-2" style="margin: 20px 54px 0;">
                    <li>
                        <button class="offer-btn" onclick="GetHoldData()"><i class="fa fa-search" aria-hidden="true"></i>@LanguageProvider.get("search")  </button>
                    </li>
                    <li>
                        <button class="offer-btn" onclick="reset()"><i class="fa fa-refresh" aria-hidden="true"></i>@LanguageProvider.get("reset") </button>
                    </li>
                    <li>
                        <button class="offer-btn" onclick="ReleaseStoneModal()"><i class="fa fa-rocket" aria-hidden="true"></i>Release</button>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="aggrid-section">
        <div class="row">
            <div class="col-xl-12">
                <!---- =============== start ag-grid =============== ---->
                <div id="myGrid" style="width: 100%;height:320px;" class="ag-theme-balham">
                </div>
                <!---- =============== end ag-grid ================= ----->
            </div>
        </div>
    </div>
</div>
<div class="modal fade social-model" id="ReleaseMsgBox">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Release Stone</h4>
                <button type="button" class="close" onclick="ClearRemoveModel()">&times;</button>
            </div>
            <div class="modal-body">
                <li>Are you sure you want to process</li>
            </div>

            <div class="modal-footer">
                <button id="btnyes" class="btn btn-secondary" onclick="ReleaseStone()" type="button">Release</button>
                <button id="btnno" class="btn btn-primary" type="button" onclick="ClearRemoveModel()">@LanguageProvider.get("cancel")</button>
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script src="~/Content/js/wow.min.js"></script>
    <script src="~/Content/js/moment.min.js"></script>
    <script src="~/Content/js/daterangepicker.min.js"></script>
    <script src="~/Content/js/ag-grid-enterprise.min.js"></script>
    <script src="~/Content/js/Pages/HoldStoneReport.js?version=1.5"></script>
    <script src="~/Content/js/Pages/detailCellRenderer.js"></script>
}

