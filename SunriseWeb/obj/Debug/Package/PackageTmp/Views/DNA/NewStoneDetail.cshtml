                                {
                                    <a href="javascript:void(0);" onclick="openpopup('All')">All</a>
                                }
                            }
                        </div>
                    </div>
                </div>
                <div class="col-sm-4" style="padding: 8px;">
                    <span>@Model.stone_ref_no</span>
                </div>
