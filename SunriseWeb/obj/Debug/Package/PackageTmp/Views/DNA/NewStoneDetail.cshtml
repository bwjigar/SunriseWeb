                                (Model.movie_url != null && Model.movie_url != ""))
                                {
                                    <a href="javascript:void(0);" onclick="Download('Video')">Video</a>
                                }
                            }

                            @if (Model != null && Model.IsOverseas == 0)
                            {
                                if (Model.view_certi_url != "" && Model.view_certi_url != null)
                                {
                                    <a href="javascript:void(0);" onclick="Download('Certificate')">Certificate</a>
                                }
                            }
                            @if (Model != null && Model.IsOverseas == 0)
                            {
                                <a href="javascript:void(0);" onclick="Download('Excel')">Excel</a>
                            }
                            else
                            {
                                @*<a href="javascript:void(0);" onclick="OverSeasExcelDownload('Excel')" class="Multidown-item">Excel</a>*@
                            }


                            @if (Model != null && Model.IsOverseas == 1)
                            {
                                if (Model != null && (Model.sImglink != null || Model.sVdoLink != null || Model.sImglink != "" || Model.sVdoLink != ""))
