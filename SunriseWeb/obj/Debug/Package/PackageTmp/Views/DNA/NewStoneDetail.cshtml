                                (Model.bPRimg == true || Model.bHBimg == true || Model.bHTimg == true || Model.bASimg == true))
                                {
                                    <a href="javascript:void(0);" onclick="openpopup('image')">Image</a>
                                }
                            }

                            @if (Model != null && Model.IsOverseas == 1)
                            {
                                if (Model.sVdoLink != "" && Model.sVdoLink != null)
                                {
                                    <a href="javascript:void(0);" onclick="openpopup('video')">Video</a>
                                }
