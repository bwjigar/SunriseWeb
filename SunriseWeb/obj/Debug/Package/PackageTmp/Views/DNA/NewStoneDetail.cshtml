                                    @*<a href="javascript:void(0);" onclick="OverSeasExcelDownload('Image')" class="Multidown-item">Image</a>
                                        <div class="dropdown-divider Down_divider"></div>*@
                                }
                            }
                            else
                            {
                                if (Model != null &&
                                (Model.bPRimg == true || Model.bHBimg == true || Model.bHTimg == true || Model.bASimg == true))
                                {
                                    <a href="javascript:void(0);" onclick="Download('Image')">Image</a>
                                }
                            }

                            @if (Model != null && Model.IsOverseas == 1)
                            {
                                if (Model.sVdoLink != "" && Model.sVdoLink != null)
                                {
                                    @*<a id="aOS_Video" href="javascript:void(0);" onclick="OverSeasExcelDownload('Video')" class="Multidown-item">Video</a>
                                        <div class="dropdown-divider Down_divider"></div>*@
                                }
