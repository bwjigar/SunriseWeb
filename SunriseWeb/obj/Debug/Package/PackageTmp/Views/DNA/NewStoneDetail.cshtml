            /*width: 50%;*/
            color: white;
            padding: 10px;
            height: 500px;
            text-align: center;
        }

        .box3 {
            /*float: left;*/
            /*width: 100%;*/
            color: white;
            padding: 10px;
            height: 580px;
            text-align: center;
        }

        .box4, .box5 {
            /*float: left;*/
            /*width: 50%;*/
            color: white;
            padding: 10px;
            height: 565px;
            text-align: center;
        }

        .box1, .box2, .box3, .box4, .box5 {
            border-top: 2px solid #003d6624;
            border-bottom: 2px solid #003d6624;
            border-left: 2px solid #003d6624;
            border-right: 2px solid #003d6624;
            background: none !important;
            color: black;
        }



        .container:after {
            clear: both;
        }

        .stonedet {
            width: 49%;
        }

        .stonedet-1 {
            width: 23%;
        }

        .stonedet-dash {
            width: 2%;
        }

        .st-al-le {
            float: left;
        }

        .st-al-ri {
            float: right;
            /*text-shadow: 1px 10px 44px #003d66, 0 0 14em white, 0 0 1.2em #dbe4e9;
            color: black;
            font: 1em "Segoe UI", Tahoma, Geneva, Verdana, sans-serif;*/
        }

        .dropbtn {
            background-color: white;
            color: #003d66;
            padding: 6px;
            font-size: 13px;
            border: none;
            font-weight: 500;
        }

        .dropdown {
            position: relative;
            display: inline-block;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            background-color: #f1f1f1;
            min-width: 121px;
            box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
            z-index: 1;
