@{
    ViewBag.Title = "Client FTP / API Portal Log";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ClassName = "search-contnt";
    <link href="~/Content/css/daterangepicker.css" rel="stylesheet" />

}
<style type="text/css">
    .ag-paging-page-summary-panel {
        left: 10px;
        position: relative;
    }

    .ag-theme-balham .ag-cell {
        white-space: normal !important;
        line-height: inherit;
        overflow-y: auto;
    }
    .Yes {
        margin-bottom: 1px;
        color: white;
        background-color: #008000ab;
        padding: 2px 5px;
        border-radius: 16px;
        font-weight: bold;
    }
    .No {
        margin-bottom: 1px;
        color: white;
        background-color: #ff0000cf;
        padding: 2px 5px;
        border-radius: 16px;
        font-weight: bold;
    }
</style>

<div class="container-fluid search_result viewcart-fluid">
    <div class="tab-content" id="myTabContent">

        <div class="tab-pane active result-page-content viewcart-page-content" id="profile-tab2" role="tabpanel" aria-labelledby="profile-tab">
            <div class="result-nav">
                <div class="row">
                    <div class="col-xl-4 col-lg-4 col-md-12 col-12">
                        <div class="viewcartdata-header">
                            <h1>Client FTP / API Portal Log</h1>
                        </div>
                    </div>

                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="column-setting-form full-acform">
                            <div class="row">
                                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6 col-12 float-left">
                                    <div class="common-order">
                                        <div class="offer-label">Date:</div>
                                        <input type="text" name="From" id="txtFromDate" value="" class="commonFromRange offer-input" autocomplete="off" />
                                        <input type="text" name="To" id="txtToDate" value="" class="commonFromRange offer-input" autocomplete="off" />
                                    </div>
                                </div>
                                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6 col-12 float-left">
                                    <div class="order-stone-id common-order">
                                        <div class="offer-label">Company Name:</div>
                                        <input id="txtMSearch" placeholder="Company Name" type="text" class="form-control common-control" autocomplete="off">
                                    </div>
                                </div>

                                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6 col-12 float-left">
                                    <div class="offer-label">&nbsp;</div>
                                    <ul class="offer-inbtn p-0 mb-2">
                                        <li style="padding-right: 5px;">
                                            <button type="button" id="btnSearch" class="offer-btn"><i class="fa fa-search" aria-hidden="true"></i> Search</button>
                                        </li>
                                        <li style="padding-right: 5px;">
                                            <button type="button" id="btnReset" class="offer-btn"><i class="fa fa-refresh" aria-hidden="true"></i> Reset</button>
                                        </li>
                                        <li>
                                            <button type="button" id="btnDistinct" class="offer-btn"><i class="fa fa-object-group" aria-hidden="true"></i> Grouping</button>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="aggrid-section viewcart">
                <div class="row">
                    <div class="col-xl-12">
                        <!---- =============== start ag-grid =============== ---->
                        <div id="myGrid" style="width: 100%;" class="ag-theme-balham">
                        </div>
                        <!---- =============== end ag-grid ================= ----->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--- end of order-history part---->
@section scripts{
    <script src="~/Content/js/moment.min.js"></script>
    <script src="~/Content/js/daterangepicker.min.js"></script>
    <script src="~/Content/js/ag-grid-enterprise.min.js"></script>
    <script src="~/Content/js/common.js"></script>
    <script src="~/Content/js/Pages/FTPAPIPortalLog.js"></script>
}




