            }

            .dv_Crt_Wgt1 {
                width: 110% !important;
            }

            .p_f_clr {
                margin-top: 0px !important;
            }

            .fncy-rst {
                margin-top: 7px !important;
            }
        }

        .f_clr_clk {
            margin-top: -11px;
            z-index: 9999;
        }

        #sym-sec0.active .carat-dropdown-main {
            display: block;
            transition: all 0.3s;
            -o-transition: all 0.3s;
            -ms-transition: all 0.3s;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
        }

        #sym-sec1.active .carat-dropdown-main {
            display: block;
            transition: all 0.3s;
            -o-transition: all 0.3s;
            -ms-transition: all 0.3s;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
        }

        #sym-sec2.active .carat-dropdown-main {
            display: block;
            transition: all 0.3s;
            -o-transition: all 0.3s;
            -ms-transition: all 0.3s;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
        }

        #sym-sec3.active .carat-dropdown-main {
            display: block;
            transition: all 0.3s;
            -o-transition: all 0.3s;
            -ms-transition: all 0.3s;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
        }

        .active_btn {
            border: 1px solid #e5e5e5;
            border-radius: 6px;
            padding: 5px 15px;
            float: left;
            margin-bottom: 5px;
            margin-right: 5px;
            transition-duration: 0.3s;
            -webkit-transition: 0.3s;
            -moz-transition: 0.3s;
            cursor: pointer;
            font-size: 10px;
            white-space: nowrap;
            font-weight: 600;
            background: #fff;
            position: relative;
            z-index: 0;
            overflow: hidden;
            margin-top: 5px;
            /*background: #003d66;
                color: #fff;*/
            box-shadow: none;
            border-radius: 6px;
        }

        .active_btn_active {
            background: #003d66;
            color: #fff;
        }

        .active_btn_inactive {
            background: #fff;
            color: #003d66;
        }

        .part2 li.active ::after {
            border: 1px solid rgba(255,255,255,0.5);
        }

        .fncy-rst {
            right: -6px;
            top: 13px;
            position: absolute;
            /*font-size: 14px;*/
            cursor: pointer;
        }

        .sym-sec .carat-dropdown-main ul li {
            display: inline-block;
            margin: -3px 0px 0px 0px;
            width: 23%;
        }

        .g-head-t {
            font-size: 12px;
            padding: 0 12px;
            color: grey;
        }

        .g-amt-t {
            font-size: 12px;
            padding: 0 12px;
            color: #003d66;
        }

        #overlay {
            position: fixed;
            display: none;
            width: 100%;
            height: 100%;
            top: 73px;
            left: 0;
            right: 0;
            bottom: 0;
            background-color: rgba(0,0,0,0.5);
            z-index: 2;
            cursor: pointer;
        }

        .tool_ag_cell {
            overflow: visible;
        }

        .tags {
            display: inline;
            position: relative;
        }

            .tags:hover:after {
                background: #003d66;
                border-radius: 5px;
                bottom: -6px;
                color: #fff;
                content: attr(gloss);
                left: -99px;
                /* padding: 0px 10px; */
                position: absolute;
                z-index: 98;
                width: 80px;
                font-weight: bold;
            }

            .tags:hover:before {
                border: solid;
                border-color: #003d66 transparent;
                border-width: 0px 7px 7px 7px;
                bottom: 4px;
                content: "";
                position: absolute;
                z-index: 99;
                margin-left: -23px;
                -webkit-transform: rotate(89deg);
            }

        #div_CERTI_TYPE ul li {
            display: inline-block;
            margin: 0px 0px 0px 1px;
            width: 12%;
        }

        #div_CERTI_TYPE {
            height: 110px;
            overflow: auto;
        }

        .crty_type_clk {
            margin-top: -11px;
            z-index: 9999;
        }
    </style>
}
