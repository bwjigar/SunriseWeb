@using SunriseWeb.Resources;
@{
    ViewBag.Title = LanguageProvider.get("search_stock");
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ClassName = "search-contnt";
    <link href="~/Content/css/daterangepicker.css" rel="stylesheet" />
    <style>
        .placeOrderDiv {
            font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol","Noto Color Emoji";
            font-size: .875rem;
            position: absolute;
            border: 1px solid #003d66;
            text-decoration: none;
            border-radius: .25rem;
            padding: 3px;
            background-color: #003d66;
            color: #fff;
            top: -36px;
            width: 81px !important;
            left: -23px;
            opacity: 0.9;
        }

            .placeOrderDiv::before {
                content: '';
                position: absolute;
                display: block;
                width: 0px;
                left: 50%;
                bottom: 0;
                border: 7px solid transparent;
                border-bottom: 0;
                border-top: 7px solid #000;
                transform: translate(-50%, calc(100%));
            }

        .showchk {
            display: block !important;
        }

        .search-page-content ul.search li a.active span.first-ico {
            color: #fff;
        }

        .search-page-content ul.search li a:hover span.first-ico {
            color: #fff;
        }

        .search-page-content ul.search li a.active .icon-image {
            background: #003d66 !important;
        }

        ul.search li a:hover .icon-image {
            background: #7ba8c5 !important;
        }

        .search-page-content .common-li li.active::before {
            background: #003d66; /*rgba(0, 61, 102,0.8);*/
        }

        .search-page-content .common-li li::before {
            background: #7ba8c5;
        }

        @@media screen and (min-width: 321px) and (max-width: 411px) {
            .search-page-content.tab-pane ul li input {
                width: 133px;
            }
        }

        @@media(max-width: 320px) {
            .search-page-content.tab-pane ul li input {
                width: 112px;
            }
        }

        @@media screen and (min-width: 412px) and (max-width: 414px) {
            .search-page-content.tab-pane ul li input {
                width: 133px;
            }
        }

        @@media screen and (min-width: 415px) and (max-width: 450px) {
            .search-page-content.tab-pane ul li input {
                width: 133px;
            }
        }

        @@media screen and (min-width: 320px) and (max-width: 778px) {
