                                                        </div>
                                                        <a href="~/Content/images/no-img1.jpg" class="enlarge_btn" title="Toggle Zoom"></a>
                                                    </div>
                                                    <a href="~/Content/images/no-img1.jpg" title="" class="img-zoom"><i class="fa fa-expand" aria-hidden="true"></i></a>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                                @if (Model != null &&
                                            (Model.bPRimg == true || Model.bHBimg == true || Model.bHTimg == true || Model.bASimg == true))
