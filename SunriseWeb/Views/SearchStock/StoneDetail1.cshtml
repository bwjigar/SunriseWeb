                                                                <li class="two">
                                                                    @if (Model != null)
                                                                    {@Model.polish}
                                                                </li>
                                                            </ul>
                                                            <ul>
                                                                <li class="one">Symmetry :</li>
                                                                <li class="two">
                                                                    @if (Model != null)
                                                                    {@Model.symm}
                                                                </li>
                                                            </ul>
