                                                                    @if (Model != null)
                                                                    {@Model.stone_ref_no}
                                                                </li>
                                                            </ul>
                                                            <ul>
                                                                <li class="one">Certi No :</li>
                                                                <li class="two">
                                                                    @if (Model != null)
                                                                    {@Model.certi_no}
                                                                </li>
                                                            </ul>
                                                            <ul>
