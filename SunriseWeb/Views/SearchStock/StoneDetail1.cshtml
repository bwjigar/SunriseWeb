                                    <div class="stone-det-big-img">
                                        <div class="dna-images">
                                            <ul class="nav nav-tabs">
                                                @if (Model.bPRimg == true)
                                                {
                                                    <li class="">
                                                        <a id="one" data-toggle="tab" href="#PR_@(Model.certi_no)_PR" class="nav-link active"><img src="https://4e0s0i2r4n0u1s0.com/img/@(Model.certi_no)/PR.jpg"></a>
                                                    </li>
                                                }
                                                @if (Model.bASimg == true)
                                                {
                                                    <li class="">
                                                        @if (Model.bPRimg == false)
                                                        {
                                                            <a id="two" data-toggle="tab" href="#AS_@(Model.certi_no)_AS" class="nav-link active"><img src="https://4e0s0i2r4n0u1s0.com/img/@(Model.certi_no)/AS.jpg"></a>
                                                        }
                                                        else
                                                        {
                                                            <a id="two" data-toggle="tab" href="#AS_@(Model.certi_no)_AS" class="nav-link"><img src="https://4e0s0i2r4n0u1s0.com/img/@(Model.certi_no)/AS.jpg"></a>
                                                        }
                                                    </li>
                                                }
                                                @if (Model.bHBimg == true)
                                                {
                                                    <li class="">
                                                        @if (Model.bASimg == false && Model.bPRimg == false)
                                                        {
                                                            <a id="three" data-toggle="tab" href="#HB_@(Model.certi_no)_HB" class="nav-link active"><img src="https://4e0s0i2r4n0u1s0.com/img/@(Model.certi_no)/HB.jpg"></a>
                                                        }
                                                        else
                                                        {
                                                            <a id="three" data-toggle="tab" href="#HB_@(Model.certi_no)_HB" class="nav-link"><img src="https://4e0s0i2r4n0u1s0.com/img/@(Model.certi_no)/HB.jpg"></a>
                                                        }
                                                    </li>
                                                }
                                                @if (Model.bHTimg == true)
                                                {
                                                    <li class="">
                                                        @if (Model.bASimg == false && Model.bPRimg == false && Model.bHBimg == false)
                                                        {
                                                            <a id="four" data-toggle="tab" href="#HT_@(Model.certi_no)_HT" class="nav-link active"><img src="https://4e0s0i2r4n0u1s0.com/img/@(Model.certi_no)/HT.jpg"></a>
                                                        }
                                                        else
                                                        {
                                                            <a id="four" data-toggle="tab" href="#HT_@(Model.certi_no)_HT" class="nav-link"><img src="https://4e0s0i2r4n0u1s0.com/img/@(Model.certi_no)/HT.jpg"></a>
                                                        }
                                                    </li>
                                                }
                                            </ul>
                                        </div>
                                    </div>
                                }

                                <div class="video">
                                    <div class="title">
                                        <span style="display:none;">v360</span>
                                        <a class="full-video">
                                            <i class="fa fa-expand" aria-hidden="true"></i>
                                        </a>
                                    </div>
                                    @if (Model != null && (Model.movie_url != null && Model.movie_url != ""))
                                    {
                                        <b style="color: #1c2f7a; ">Stone Video Link : </b><a href="@Model.movie_url" target="_blank" style="color: #1c2f7a;">Click Here...</a>
                                        <iframe height="350" frameborder="0" id="video1" src="@Model.movie_url&amp;autoplay=1" style="text-align: center; width: 100%;"></iframe>
                                    }
                                    else
                                    {
                                        <img src="/Content/images/no-video-available.png" width="100%" height="400">
                                    }
                                </div>

                            </div>
                            <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
                                @*<div class="stone-det-big-img p-0" ng-hide="CertDiv">
                                    <a data-fancybox="gallery" id="aCertificateUrl" href="http://rushidiamhk.com/softimg/certimg/20804-GIA.jpg">
                                        <img id="lblCertificateUrl2" style="width: 100%;" src="http://rushidiamhk.com/softimg/certimg/20804-GIA.jpg">
                                    </a>
                                </div>*@
                                <div class="stone-det-big-img p-0" ng-hide="CertDiv">
                                    @if (Model != null)
                                    {
                                        <iframe id="CertiFrame" width="100%" height="600" frameborder="0" src="@Model.view_certi_url" style="text-align: center; width: 100%;"></iframe>
                                    }
                                    else
                                    {
                                        <iframe id="CertiFrame" width="100%" height="600" frameborder="0" src="" style="text-align: center; width: 100%;"></iframe>
                                    }
                                </div>


                                <div>
                                    <div class="stone-detail-bx-main download-box-main">
                                        <div class="stone-detail-head">
                                            Download
                                        </div>
                                        <div class="download-main">
                                            <ul>
                                                <input type="hidden" id="sendtype" value="" />
                                                @if (Model != null &&
                                                    (Model.bPRimg == true || Model.bHBimg == true || Model.bHTimg == true || Model.bASimg == true))
                                                {
                                                    <li onclick="openpopup('image')"><i class="fa fa-whatsapp" aria-hidden="true"></i> IMAGE</li>
                                                }
                                                @if (Model != null &&
                                                        (Model.movie_url != null && Model.movie_url != ""))
                                                {
                                                    <li onclick="openpopup('video')"><i class="fa fa-whatsapp" aria-hidden="true"></i> VIDEO</li>
                                                }
                                                @if (Model != null && Model.view_certi_url != null)
                                                {
                                                    <li onclick="openpopup('certi')"><i class="fa fa-whatsapp" aria-hidden="true"></i> CERTIFICATE</li>
                                                }
                                                <li onclick="Download('Excel')"><i class="fa fa-file-excel-o" aria-hidden="true"></i> EXCEL</li>
                                                @if (Model != null &&
                                                        (Model.bPRimg == true || Model.bHBimg == true || Model.bHTimg == true || Model.bASimg == true))
                                                {
                                                    <li onclick="Download('Image')"><i class="fa fa-download" aria-hidden="true"></i> IMAGE</li>
                                                }
                                                @if (Model != null &&
                                                    (Model.movie_url != null && Model.movie_url != ""))
                                                {
                                                    <li onclick="Download('Video')"><i class="fa fa-download" aria-hidden="true"></i> VIDEO</li>
                                                }
                                                @if (Model != null && Model.view_certi_url != null)
                                                {
                                                    <li onclick="Download('Certificate')"><i class="fa fa-download" aria-hidden="true"></i> CERTIFICATE</li>
                                                }
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-3 col-sm-12">

                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
