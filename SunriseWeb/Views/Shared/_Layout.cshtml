                var link = "";
                var obj = {};
                $.ajax({
                    url: "@Url.Action("Chk_StockDisc_Avail","Dashboard")",
                    type: "POST",
                    data: null,
                    success: function (data, textStatus, jqXHR) {
                        if (data.Data != null) {
                            if (data.Data[0].Status == true) {
                                link = "@Url.Action("StockDownload","Dashboard")";
                                obj.UsedFor = "Download";
                            }
                            else if (data.Data[0].Status == false) {
                                link = "@Url.Action("TotalStockDownload","Dashboard")";
                            }
                            if (link != "") {
                                $.ajax({
                                    url: link,
                                    type: "POST",
                                    data: obj,
                                    success: function (data, textStatus, jqXHR) {
                                        $('.loading-overlay-image-container').hide();
                                        $('.loading-overlay').hide();
                                        location.href = data;
                                    },
                                    error: function (jqXHR, textStatus, errorThrown) {

                                    }
                                });
                            }
                        }
                    },
                    error: function (jqXHR, textStatus, errorThrown) {

                    }
                });
            });
            LoginCheck();
            if ($("#hdnIsSubUser").val() == "True") {
                PagePermission();
            }
        });
        function PagePermission() {
            var pathname = window.location.pathname.toLowerCase(), search = window.location.search;
            if ((pathname == "/searchstock/search" || pathname == "/settings/columnssettings") && $("#hdnSearchStock").val() == "False") {
                if (search == "") {
                    window.location.href = "/Dashboard";
                }
            }
            if (pathname == "/order/orderhistory" && $("#hdnOrderHisrory").val() == "False") {
                window.location.href = "/Dashboard";
            }
            if ((pathname == "/wishlist/index" || pathname == "/wishlist") && $("#hdnMyWishlist").val() == "False") {
                window.location.href = "/Dashboard";
            }
            if (pathname == "/wishlist/admin_wishlist") {
                window.location.href = "/Dashboard";
            }
            if ((pathname == "/cart/index" || pathname == "/cart") && $("#hdnMyCart").val() == "False") {
                window.location.href = "/Dashboard";
            }
            if (pathname == "/searchstock/quicksearch" && $("#hdnQuickSearch").val() == "False") {
                window.location.href = "/Dashboard";
            }
        }
        function MoveToErrorPage(data_Status) {
            var flag = false;
            data_Status = String(data_Status);

            if (data_Status == "0") {
                flag = true;
            }
            else if (data_Status.toUpperCase() == "FAIL") {
                flag = true;
            }

            if (flag == true) {
                window.location.href = "/Error";
            }
        }
        var Stone_List = "";
        function CommaSeperatedStone_list(e) {
            if (!$("#content-desktop").is(":hidden")) {
                var data = document.getElementById("search_desktop").value;
                var lines = data.split(' ');
                document.getElementById("search_desktop").value = lines.join(',');
                //document.getElementById("search_desktop").value = document.getElementById("search_desktop").value.replace(/(?:\r\n|\r|\n)/g, ',');
                Stone_List = document.getElementById("search_desktop").value;
            }
            if (!$("#content-mobile").is(":hidden")) {
                var data = document.getElementById("search_mobile").value;
                var lines = data.split(' ');
                document.getElementById("search_mobile").value = lines.join(',');
                Stone_List = document.getElementById("search_mobile").value;
            }
            var key = e.which;
            if (key == 13)  // the enter key code
            {
                SmartSearch();
            } else {
                return false;
            }
        }

        function SmartSearch() {
            if (Stone_List != "") {
                if (getParameterByName('type') != "SmartSearch") {
                    $.ajax({
                        type: 'POST',
                        url: '/SearchStock/SmartSearchURLSet',
                        dataType: "json",
                        data: { URL: window.location.href },
                        success: function (data) {

                        }
                    });
                }
                $.ajax({
                    type: 'POST',
                    url: '/SearchStock/StoneSmartSearch',
                    dataType: "json",
                    data: { StoneList: Stone_List },
                    success: function (data) {
                        Stone_List = "";
                        var Type = "SmartSearch";
                        window.location = '/SearchStock/Search?type=' + Type;
                    }
                });
            }
        }
        function LanguageChange(langCode) {
            $.ajax({
                url: "/Dashboard/ChangeLanguage",
                async: false,
                type: "POST",
                data: { LangCode: langCode },
                success: function (data, textStatus, jqXHR) {
                    window.location.reload();
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    window.location.reload();
                }
            });
        }

        function GetLoginUrl() {
            return '@Url.Action("Logout","Login")';
        }
        function getParameterByName(name) {
            var url = window.location.href;
            name = name.replace(/[\[\]]/g, '\\$&');
            var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, ' '));
        }

        function NotifyGet_User() {
            $.ajax({
                type: 'POST',
                url: '/User/NotifyGet_User',
                dataType: "json",
                data: { iUserid: '' },
                success: function (data) {
                    var _html = "";
                    if (data.Data.length != 0) {
                        $("#Notify-count").show();
                        $("#Notify-count").html(data.Data.length);
                        for (var i = 0; i < data.Data.length; i++) {
                            _html += "<a class='dropdown-item d-flex align-items-center' id='a_Alerts" + data.Data[i].Id + "'>";
                            _html += "<div style='width: 500px !important;'>";
                            _html += "<div>";
                            _html += "<span class='small text-gray-500'>" + data.Data[i].ValidityFromDate + "</span>";
                            _html += "<span class='dismiss pull-right' onclick=\"DismissClick('" + data.Data[i].Id + "')\">Dismiss</span>";
                            _html += "<span class='skip pull-right' onclick=\"SkipClick('" + data.Data[i].Id + "')\" style='margin-right: 2px;'>Skip</span>";
                            _html += "</div>";
                            _html += "<span style='font-size: 16px;'>" + data.Data[i].Message.replace(/'/g, ' &_ ') + "</span>";
                            _html += "</div>";
                            _html += "</a>";
                        }
                    }
                    else {
                        $("#Notify-count").hide();
                        _html += "<a class='dropdown-item d-flex align-items-center'>";
                        _html += "<div style='width: 500px !important;'>";
                        _html += "<span style='font-size: 16px;'>No Notification Found !</span>";
                        _html += "</div>";
                        _html += "</a>";
                    }
                    $("#div_Alerts_Center").html(_html.replace(/ &_ /g, "'"));
                }
            });
        }
        function onclickAlertsCenter() {
            if (IsAlertsCenterOpen == 0) {
                NotifyGet_User();
                $("#AlertsCenter").show();
                IsAlertsCenterOpen = 1;
            }
            else if (IsAlertsCenterOpen == 1) {
                $("#AlertsCenter").hide();
                IsAlertsCenterOpen = 0;
            }
        }
        function DismissClick(Id) {
            $.ajax({
                url: "/User/SaveNotifyList",
                type: "POST",
                data: {
                    FromDate: '',
                    ToDate: '',
                    Message: '',
                    UserIdList: '',
                    NotifyId: Id
                },
                success: function (data, textStatus, jqXHR) {
                    if (data.Status == "1") {
                        $("#div_Alerts_Center #a_Alerts" + Id).remove();
                        if ($('#div_Alerts_Center:empty').length == 1) {
                            $("#Notify-count").hide();
                            var _html = "<a class='dropdown-item d-flex align-items-center'>";
                            _html += "<div style='width: 500px !important;'>";
                            _html += "<span>No Notification Found !</span>";
                            _html += "</div>";
                            _html += "</a>";
                            $("#div_Alerts_Center").html(_html.replace(/ &_ /g, "'"));
                        }
                        else {
                            $("#Notify-count").show();
                            $("#Notify-count").html(parseInt($("#Notify-count").html()) - 1);
                        }
                    } else {
                        toastr.error(data.Message);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $('.loading-overlay-image-container').hide();
                    $('.loading-overlay').hide();
                }
            });
        }
        function SkipClick(Id) {
            $("#AlertsCenter").hide();
            IsAlertsCenterOpen = 0;
        }
        function fn60sec() {
            $.ajax({
                url: "/Login/LoginCheck",
                type: "POST",
                data: null,
                success: function (data, textStatus, jqXHR) {
                    if (data != "OK") {
                        window.location.href = "/Login";
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                }
            });
        }
        function LoginCheck() {
            fn60sec();
            setInterval(fn60sec, 60 * 1000);
        }
